{
    "regex": "[a-z]+",
    "special_characters": {
        "escape": "\n",
        "backspace": "\u007f",
        "tab": "\t",
        "space": " ",
        "empty": ""
    },
    "stop_terms_fraction": 1e-2,
    "max_completions": 10,
    "max_images": 3,
    "sequencediff_cutoff": 0.8,
    "semantic_cutoff": 0.7,
    "n_semantic_neighbors": 1,
    "max_predictions": 1,
    "max_corrections": 1
}