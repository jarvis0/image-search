{
    "regex": "[a-z]+",
    "special_characters": {
        "escape": "\n",
        "backspace": "\u007f",
        "tab": "\t",
        "space": " ",
        "empty": ""
    },
    "stop_terms_fraction": 1e-2,
    "max_completions": 10,
    "max_images": 3,
    "sequence_similarity_cutoff": 0.8,
    "max_sequence_similarities": 3,
    "semantic_similarity_cutoff": 0.6,
    "max_semantic_similarities": 1,
    "max_term_predictions": 1,
    "max_term_corrections": 1,
    "max_term_completions": 1
}